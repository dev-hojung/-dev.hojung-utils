{
  "name": "@dev.hojung/utils",
  "version": "0.0.1",
  "description": "utils",
  "author": {
    "name": "dev.hojung",
    "email": "dev.hojung@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-hojung/utils_package"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "ts-node": "node --experimental-specifier-resolution=node --loader ts-node/esm index.ts",
    "test": "mocha"
  },
  "keywords": [
    "node",
    "utils",
    "javascript"
  ],
  "devDependencies": {
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.9.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
